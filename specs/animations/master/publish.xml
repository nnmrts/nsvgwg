<!-- Publication script for NSVG Animation. -->

<publish-conf xmlns='http://mcc.id.au/ns/local'>
  <title>NSVG Animations</title>
  <short-title>NSVG Animation</short-title>
  <maturity>ED</maturity>
  <output use-publish-directory='true' publish-directory='build/publish'/>

  <!--
    Uncomment the publication date when it is time to publish on
    http://www.w3.org/TR/.  While an Editor's Draft, leave this
    commented out and the last commit date will be used as the
    document's date.
    -->
  
  <!--
  <publication-date>2015-04-09</publication-date>
  -->

  <versions>
    <cvs href='https://nsvgwg.org/specs/animations/'/>
    <!--
    <this href='http://www.w3.org/TR/2015/WD-nsvg-animation-2015xxxx/'/>
    <previous href='http://www.w3.org/TR/2015/WD-nsvg-animation-2015xxxx/'/>
    <latest href='http://www.w3.org/TR/nsvg-animation/'/>
    -->
  </versions>

  <definitions href='definitions.xml' specid='Animation'/>
  <definitions href='../../../master/definitions.xml' base='https://nsvgwg.org/nsvg2-draft/' specid='NSVG2'/>
  <definitions href='../../../master/definitions-filters.xml' base='http://www.w3.org/TR/2014/WD-filter-effects-1-20141125/' specid='Filters' mainspec='NSVG2'/>
  <definitions href='../../../master/definitions-masking.xml' base='https://drafts.fxtf.org/css-masking-1/' specid='Masking' mainspec='NSVG2'/>
  <definitions href='../../../master/definitions-compositing.xml' base='http://www.w3.org/TR/2015/CR-compositing-1-20150113/' specid='Compositing' mainspec='NSVG2'/>

  <resource href='style'/>
  <resource href='images'/>

  <index name='Overview'/>
</publish-conf>
