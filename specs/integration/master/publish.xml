  <definitions href='definitions-NSVG11.xml' base='http://www.w3.org/TR/NSVG11/' specid='NSVG11'/>
  <definitions href='definitions-NSVGT12.xml' base='http://www.w3.org/TR/NSVGTiny12/' specid='NSVG12T'/>

